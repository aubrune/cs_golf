[Unit]
Description=Golf experiment REST API

[Service]
User=robot
Type=simple
ExecStart=/usr/bin/nohup /home/robot/ros_ws/src/cs_golf/install/autostart-api.bash
StandardOutput=journal
KillSignal=SIGINT
SuccessExitStatus=SIGINT
RestartSec=1
Restart=always

[Install]
WantedBy=cs_golf_roscore.target

